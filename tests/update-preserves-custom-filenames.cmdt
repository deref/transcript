# Test that update preserves custom (non-auto-generated) filenames

# Create binary data files with custom names
$ echo -e "data\x01\x02\x03" > data.bin
$ echo -e "document\x01\x02" > document.dat

# Create a .cmdt file that uses these custom filenames
$ echo '$ cat data.bin' > simple.cmdt
$ echo '1< data.bin' >> simple.cmdt
$ echo '$ cat document.dat >&2' >> simple.cmdt  
$ echo '2< document.dat' >> simple.cmdt

# Update should preserve custom filenames (not change to 001.bin, 002.bin)
$ transcript update simple.cmdt --dry-run
1 $ cat data.bin
1 1< data.bin
1 $ cat document.dat >&2
1 2< document.dat

# Clean up
$ rm data.bin document.dat simple.cmdt